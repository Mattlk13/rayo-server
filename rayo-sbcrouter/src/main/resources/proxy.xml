<?xml version="1.0" encoding="UTF-8"?>
<!-- <!DOCTYPE Server SYSTEM "server.dtd"> -->

<server>
  <domains>
    <domain>127.0.0.1</domain>
    <domain>localhost</domain>
    <domain default="true">voxeo.com</domain>
  </domains>
  <proxy-service enable="true" version="1.0">
    <recurse>false</recurse>
    <record-route>true</record-route>
    <add-path-to-register>true</add-path-to-register>
    <parallel>false</parallel>
    <stateful>true</stateful>
    <broadcast-message-to-resource-list>false</broadcast-message-to-resource-list>
    <supervised>true</supervised>
    <sequential-search-timeout>32</sequential-search-timeout>
    <session-timeout>120</session-timeout>
    <routers>
    	<router name="RayoRouter" class="com.rayo.sbcrouter.RayoRouter" CassandraHost="localhost" CassandraPort="9160" />
    </routers>
  </proxy-service>
</server>