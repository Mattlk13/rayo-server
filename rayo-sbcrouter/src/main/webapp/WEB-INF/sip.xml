<?xml version="1.0" encoding="UTF-8"?>

<sip-app xmlns="http://www.jcp.org/xml/ns/sipservlet" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:javaee="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://www.jcp.org/xml/ns/sipservlet http://www.jcp.org/xml/ns/sipservlet/sip-app_1_1.xsd http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
	version="1.1">
	<app-name>RayoSBCRouter</app-name>
	<servlet>
		<javaee:servlet-name>RayoSBCRouter</javaee:servlet-name>
		<javaee:servlet-class>
			com.rayo.sbcrouter.SBCRouterServlet</javaee:servlet-class>
		<javaee:load-on-startup>1</javaee:load-on-startup>

		<javaee:init-param>
			<javaee:param-name>CassandraHost</javaee:param-name>
			<javaee:param-value>127.0.0.1</javaee:param-value>
		</javaee:init-param>
		
		<javaee:init-param>
			<javaee:param-name>CassandraPort</javaee:param-name>
			<javaee:param-value>9160</javaee:param-value>
		</javaee:init-param>
	</servlet>
</sip-app>
