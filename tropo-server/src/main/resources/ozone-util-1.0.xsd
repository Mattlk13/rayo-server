<?xml version="1.0" encoding="UTF-8"?>
<schema xmlns="http://www.w3.org/2001/XMLSchema" 
	targetNamespace="urn:xmpp:ozone:util:1" 
	xmlns:tns="urn:xmpp:ozone:util:1" 
	elementFormDefault="qualified">

    <!-- Import SSML Types -->
    <include schemaLocation="http://www.w3.org/TR/speech-synthesis/synthesis-core.xsd" />

    <complexType name="ssmlType">
        <complexContent>
            <extension base="tns:speak">
                <attribute name="voice" type="string" />
            </extension>
        </complexContent>
    </complexType>

</schema>